package variableTask02;

public class variableTask02 {
	public static void main(String[] args) {
//      5, 7845.15, false, B, 85.10F, 자바, S, 98590, D, true, 공백문자
//      저장공간 2개만 사용해서 모든 값을 출력하기
//      datas, numbers
		
		int num01 = 0;
		long num02 = 0;
		float num03 = 0.0F;
		double numbers = 0.0;
		boolean data = false;
		char data02 = ' ';
		String datas = null;
		
		
		datas = "B" ;
		System.out.println(datas);
		
		
		
		datas = "B" false "자바" "S" "D" true "공백문자";
		datas = "B" false "자바" "S" "D" true "공백문자";
		
		numbers = 5, 7845.15, 85.10F, 98590;
		
		System.out.println(datas);
		System.out.println(numbers);
		
		
		
		
	}
}
