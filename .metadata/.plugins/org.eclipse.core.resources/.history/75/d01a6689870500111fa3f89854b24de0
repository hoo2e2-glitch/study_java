package castingTest;

public class CastingTest {
	public static void main(String[] args) {
		// 형변환
		
		// 자동형변환
		// 정수 + 정수 = 정수
		System.out.println(1 + 2);
		System.out.println(10 / 3);
		
		// 정수 + 실수 = 실수(double)
		System.out.println(1 + 0.5);
		System.out.println(5.0 / 2);
		
		
		
		
	}

}
