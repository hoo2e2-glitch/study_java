package formatTask02;

public class FormatTask02 {
	public static void main(String[] args) {
		
//      %자릿수d
//      %5d
      
//      국어점수, 영어점수, 수학점수를 받아서 서식에 맞게 출력
//      이름: 이규학
//      수학:    20점
//      국어:    40점
//      영어:    15점
//      총점:    75점
		
		int num1 = 20, num2 = 40, num3 = 15, num4 = 75;
		String name = "이규학";
		
		System.out.printf("이름: %5d", name);
		System.out.printf("수학: %5d", num1);
		System.out.printf("국어: %5d", num2);
		System.out.printf("영어: %5d", num3);
		System.out.printf("총점: %5d", num4);
		
		
	}

}
